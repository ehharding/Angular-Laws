{
  "root" : true,
  "env" : { "browser" : true, "jasmine" : true, "node" : true },
  "parser" : "@typescript-eslint/parser",
  "parserOptions" : {
    "ecmaFeatures" : {
      "arrowFunctions" : true,
      "binaryLiterals" : true,
      "octalLiterals" : true,
      "blockBindings" : true,
      "classes" : true,
      "defaultParams" : true,
      "destructuring" : false,
      "experimentalObjectRestSpread" : false,
      "forOf" : true,
      "generators" : false,
      "globalReturn" : false,
      "impliedStrict" : true,
      "jsx" : false,
      "modules" : true,
      "objectLiteralComputedProperties" : true,
      "objectLiteralDuplicateProperties" : false,
      "objectLiteralShorthandMethods" : true,
      "objectLiteralShorthandProperties" : true,
      "regexUFlag" : true,
      "regexYFlag" : true,
      "restParams" : true,
      "spread" : true,
      "superInFunctions" : true,
      "templateStrings" : true,
      "unicodeCodePointEscapes" : true
    },
    "allowReserved" : false,
    "ecmaVersion" : 2022,
    "project" : "tsconfig.eslint.json",
    "sourceType" : "module"
  },
  "extends" : ["eslint:recommended", "plugin:@typescript-eslint/recommended", "plugin:import/recommended", "plugin:node/recommended"],
  "ignorePatterns" : ["*.html", "dist", "dist-production", "node_modules"],
  "plugins" : ["@typescript-eslint", "import", "node", "tsdoc"],
  "rules" : {
    "accessor-pairs" : ["warn", { "enforceForClassMembers" : true, "getWithoutSet" : true, "setWithoutGet" : true }],
    "array-bracket-newline" : ["off", "always", { "multiline" : true, "minItems" : 0 }],
    "array-bracket-spacing" : ["warn", "never", { "arraysInArrays" : false, "objectsInArrays" : false, "singleValue" : false }],
    "array-callback-return" : ["warn", { "allowImplicit" : false, "checkForEach" : true }],
    "array-element-newline" : ["off", "always", { "multiline" : true, "minItems" : 0 }],
    "arrow-body-style" : ["off", "as-needed", { "requireReturnForObjectLiteral" : true }],
    "arrow-parens" : ["warn", "as-needed", { "requireForBlockBody" : true }],
    "arrow-spacing" : ["warn", { "before" : true, "after" : true }],
    "block-scoped-var" : ["warn"],
    "block-spacing" : ["warn", "always"],
    "brace-style" : ["off", "1tbs", { "allowSingleLine" : false }],
    "camelcase" : ["warn", { "ignoreDestructuring" : false, "ignoreGlobals" : false, "ignoreImports" : false, "properties" : "always", "allow" : [] }],
    "capitalized-comments" : ["warn", "always", { "ignoreConsecutiveComments" : true, "ignoreInlineComments" : true }],
    "class-methods-use-this" : ["off", { "enforceForClassFields" : true, "exceptMethods" : [] }],
    "comma-dangle" : ["off", {
      "arrays" : "never",
      "exports" : "never",
      "functions" : "never",
      "imports" : "never",
      "objects" : "never"
    }],
    "comma-spacing" : ["off", { "before" : false, "after" : true }],
    "comma-style" : ["warn", "last", {
      "exceptions" : {
        "ArrayExpression" : false,
        "ArrayFunctionExpression" : false,
        "ArrayPattern" : false,
        "CallExpression" : false,
        "FunctionDeclaration" : false,
        "FunctionExpression" : false,
        "ImportDeclaration" : false,
        "NewExpression" : false,
        "ObjectExpression" : false,
        "ObjectPattern" : false,
        "VariableDeclaration" : false
      }
    }],
    "complexity" : ["warn", { "max" : 10 }],
    "computed-property-spacing" : ["warn", "never", { "enforceForClassMembers" : true }],
    "consistent-return" : ["warn", { "treatUndefinedAsUnspecified" : false }],
    "consistent-this" : ["warn", "that"],
    "constructor-super" : ["warn"],
    "curly" : ["warn", "all"],
    "default-case-last" : ["warn"],
    "default-case" : ["warn", { "commentPattern" : "/^no default$/i" }],
    "default-param-last" : ["off"],
    "dot-location" : ["warn", "object"],
    "dot-notation" : ["off", { "allowKeywords" : true, "allowPrivateClassPropertyAccess" : false, "allowProtectedClassPropertyAccess" : false }],
    "eol-last" : ["warn", "always"],
    "eqeqeq" : ["warn", "smart"],
    "for-direction" : ["warn"],
    "func-call-spacing" : ["off", "never"],
    "func-name-matching" : ["warn", "always", { "considerPropertyDescriptor" : false, "includeCommonJSModuleExports" : false }],
    "func-names" : ["warn", "always", { "generators" : "as-needed" }],
    "func-style" : ["warn", "declaration", { "allowArrowFunctions" : true }],
    "function-call-argument-newline" : ["off", "always"],
    "function-paren-newline" : ["warn", "multiline-arguments"],
    "generator-star-spacing" : ["warn", { "before" : true, "after" : true }],
    "getter-return" : ["warn", { "allowImplicit" : false }],
    "grouped-accessor-pairs" : ["warn", "getBeforeSet"],
    "guard-for-in" : ["warn"],
    "id-denylist" : ["warn", "callback", "cb", "e", "err"],
    "id-length" : ["warn", { "min" : 2, "max" : 50, "properties" : "always", "exceptions" : ["tmp", "_"] }],
    "id-match" : ["off", "^[a-z]+([A-Z][a-z]+)*$", { "classFields" : true, "ignoreDestructuring" : false, "onlyDeclarations" : false, "properties" : true }],
    "implicit-arrow-linebreak" : ["off", "below"],
    "indent" : ["off", 2, {
      "flatTernaryExpressions" : false,
      "ignoreComments" : false,
      "offsetTernaryExpressions" : false,
      "outerIIFEBody" : 2,
      "ArrayExpression" : 1,
      "ImportDeclaration" : 1,
      "MemberExpression" : 2,
      "ObjectExpression" : 1,
      "SwitchCase" : 1,
      "CallExpression" : { "arguments" : "first" },
      "FunctionDeclaration" : { "body" : 1, "parameters" : 2 },
      "FunctionExpression" : { "body" : 1, "parameters" : "first" },
      "StaticBlock" : { "body" : 1 },
      "VariableDeclarator" : { "const" : 2, "let" : 2, "var" : 2 },
      "ignoredNodes" : []
    }],
    "init-declarations" : ["off", "always"],
    "jsx-quotes" : ["warn", "prefer-double"],
    "key-spacing" : ["warn", {
      "multiLine" : { "beforeColon" : true, "afterColon" : true, "mode" : "strict" },
      "singleLine" : { "beforeColon" : true, "afterColon" : true, "mode" : "strict" }
    }],
    "keyword-spacing" : ["off", { "before" : true, "after" : true }],
    "line-comment-position" : ["off", { "applyDefaultIgnorePatterns" : true, "ignorePattern" : "", "position" : "above" }],
    "linebreak-style" : ["warn", "windows"],
    "lines-around-comment" : ["warn", {
      "beforeBlockComment" : false,
      "afterBlockComment" : false,
      "beforeLineComment" : false,
      "afterLineComment" : false,
      "allowBlockStart" : false,
      "allowBlockEnd" : false,
      "allowObjectStart" : true,
      "allowObjectEnd" : false,
      "allowArrayStart" : false,
      "allowArrayEnd" : false,
      "allowClassStart" : true,
      "allowClassEnd" : false,
      "applyDefaultIgnorePatterns" : true,
      "ignorePattern" : ""
    }],
    "lines-around-directive" : ["warn", { "before" : "never", "after" : "always" }],
    "lines-between-class-members" : ["off", "always", { "exceptAfterSingleLine" : true }],
    "logical-assignment-operators" : ["warn", "always", { "enforceForIfStatements" : true }],
    "max-classes-per-file" : ["warn", { "ignoreExpressions" : false, "max" : 1 }],
    "max-depth" : ["warn", { "max" : 4 }],
    "max-len" : ["warn", {
      "ignoreComments" : false,
      "ignoreRegExpLiterals" : true,
      "ignoreStrings" : true,
      "ignoreTemplateLiterals" : true,
      "ignoreTrailingComments" : false,
      "ignoreUrls" : true,
      "code" : 175,
      "comments" : 175,
      "tabWidth" : 2,
      "ignorePattern" : ""
    }],
    "max-lines-per-function" : ["warn", { "IIFEs" : false, "skipBlankLines" : false, "skipComments" : false, "max" : 100 }],
    "max-lines" : ["warn", { "skipBlankLines" : false, "skipComments" : false, "max" : 1000 }],
    "max-nested-callbacks" : ["warn", { "max" : 5 }],
    "max-params" : ["warn", { "max" : 10 }],
    "max-statements-per-line" : ["warn", { "max" : 2 }],
    "max-statements" : ["warn", { "max" : 150 }],
    "multiline-comment-style" : ["warn", "starred-block"],
    "multiline-ternary" : ["warn", "never"],
    "new-cap" : ["warn", {
      "capIsNew" : false,
      "newIsCap" : true,
      "properties" : true,
      "capIsNewExceptionPattern" : "",
      "newIsCapExceptionPattern" : "",
      "capIsNewExceptions" : [],
      "newIsCapExceptions" : []
    }],
    "new-parens" : ["warn", "always"],
    "newline-per-chained-call" : ["warn", { "ignoreChainWithDepth" : 5 }],
    "no-alert" : ["warn"],
    "no-array-constructor" : ["warn"],
    "no-await-in-loop" : ["warn"],
    "no-bitwise" : ["warn", { "int32Hint" : false, "allow" : [] }],
    "no-caller" : ["warn"],
    "no-case-declarations" : ["warn"],
    "no-class-assign" : ["warn"],
    "no-compare-neg-zero" : ["warn"],
    "no-cond-assign" : ["warn", "except-parens"],
    "no-confusing-arrow" : ["warn", { "allowParens" : true, "onlyOneSimpleParam" : true }],
    "no-console" : ["warn", { "allow" : ["error"] }],
    "no-const-assign" : ["warn"],
    "no-constant-binary-expression" : ["warn"],
    "no-constant-condition" : ["warn", { "checkLoops" : true }],
    "no-constructor-return" : ["warn"],
    "no-continue" : ["warn"],
    "no-control-regex" : ["warn"],
    "no-debugger" : ["warn"],
    "no-delete-var" : ["warn"],
    "no-div-regex" : ["warn"],
    "no-dupe-args" : ["warn"],
    "no-dupe-class-members" : ["off"],
    "no-dupe-else-if" : ["warn"],
    "no-dupe-keys" : ["warn"],
    "no-duplicate-case" : ["warn"],
    "no-duplicate-imports" : ["warn", { "includeExports" : true }],
    "no-else-return" : ["warn", { "allowElseIf" : false }],
    "no-empty-character-class" : ["warn"],
    "no-empty-function" : ["off", { "allow" : ["arrowFunctions", "constructors"] }],
    "no-empty-pattern" : ["warn"],
    "no-empty-static-block" : ["warn"],
    "no-empty" : ["warn", { "allowEmptyCatch" : false }],
    "no-eq-null" : ["warn"],
    "no-eval" : ["warn", { "allowIndirect" : false }],
    "no-ex-assign" : ["warn"],
    "no-extend-native" : ["warn", { "exceptions" : [] }],
    "no-extra-bind" : ["warn"],
    "no-extra-boolean-cast" : ["warn", { "enforceForLogicalOperands" : true }],
    "no-extra-label" : ["warn"],
    "no-extra-parens" : ["off", "all", {
      "conditionalAssign" : true,
      "returnAssign" : true,
      "enforceForArrowConditionals" : true,
      "enforceForFunctionPrototypeMethods" : true,
      "enforceForNewInMemberExpressions" : true,
      "enforceForSequenceExpressions" : true,
      "nestedBinaryExpressions" : false,
      "allowParensAfterCommentPattern" : "",
      "ignoreJSX" : "none"
    }],
    "no-extra-semi" : ["off"],
    "no-fallthrough" : ["warn", { "allowEmptyCase" : false, "commentPattern" : "/^break[\\s\\w]*omitted$/i" }],
    "no-floating-decimal" : ["warn"],
    "no-func-assign" : ["warn"],
    "no-global-assign" : ["warn", { "exceptions" : [] }],
    "no-implicit-coercion" : ["warn", { "boolean" : true, "number" : true, "string" : true, "disallowTemplateShorthand" : false, "allow" : [] }],
    "no-implicit-globals" : ["warn", { "lexicalBindings" : false }],
    "no-implied-eval" : ["off"],
    "no-import-assign" : ["warn"],
    "no-inline-comments" : ["off", { "ignorePattern" : "webpackChunkName:\\\\s.+" }],
    "no-inner-declarations" : ["warn", "both"],
    "no-invalid-regexp" : ["warn", { "allowConstructorFlags" : [] }],
    "no-invalid-this" : ["off", { "capIsConstructor" : true }],
    "no-irregular-whitespace" : ["warn", { "skipComments" : true, "skipRegExps" : false, "skipStrings" : true, "skipTemplates" : true }],
    "no-iterator" : ["warn"],
    "no-label-var" : ["warn"],
    "no-labels" : ["warn", { "allowLoop" : false, "allowSwitch" : false }],
    "no-lone-blocks" : ["warn"],
    "no-lonely-if" : ["warn"],
    "no-loop-func" : ["off"],
    "no-loss-of-precision" : ["off"],
    "no-magic-numbers" : ["off", {
      "detectObjects" : true,
      "enforceConst" : false,
      "ignoreArrayIndexes" : false,
      "ignoreClassFieldInitialValues" : true,
      "ignoreDefaultValues" : true,
      "ignore" : [-2, -1, 0, 1, 2]
    }],
    "no-misleading-character-class" : ["warn"],
    "no-mixed-operators" : ["warn", {
      "allowSamePrecedence" : true,
      "groups" : [
        ["+", "-", "*", "/", "%", "**"],
        ["&", "|", "^", "~", "<<", ">>", ">>>"],
        ["==", "!=", "===", "!==", ">", ">=", "<", "<="],
        ["&&", "||"],
        ["in", "instanceof"]
      ]
    }],
    "no-mixed-spaces-and-tabs" : ["warn", "smart-tabs"],
    "no-multi-assign" : ["warn", { "ignoreNonDeclaration" : false }],
    "no-multi-spaces" : ["warn", { "ignoreEOLComments" : true, "exceptions" : { "Property" : true } }],
    "no-multi-str" : ["warn"],
    "no-multiple-empty-lines" : ["warn", { "max" : 1, "maxBOF" : 0, "maxEOF" : 1 }],
    "no-negated-condition" : ["warn"],
    "no-nested-ternary" : ["warn"],
    "no-new-func" : ["warn"],
    "no-new-native-nonconstructor" : ["warn"],
    "no-new-object" : ["warn"],
    "no-new-symbol" : ["warn"],
    "no-new-wrappers" : ["warn"],
    "no-new" : ["warn"],
    "no-nonoctal-decimal-escape" : ["warn"],
    "no-obj-calls" : ["warn"],
    "no-octal-escape" : ["warn"],
    "no-octal" : ["warn"],
    "no-param-reassign" : ["warn", { "props" : true, "ignorePropertyModificationsFor" : [], "ignorePropertyModificationsForRegex" : [] }],
    "no-plusplus" : ["warn", { "allowForLoopAfterthoughts" : false }],
    "no-promise-executor-return" : ["warn"],
    "no-proto" : ["warn"],
    "no-prototype-builtins" : ["warn"],
    "no-redeclare" : ["off", { "builtinGlobals" : true }],
    "no-regex-spaces" : ["warn"],
    "no-restricted-exports" : ["warn", { "restrictedNamedExports" : [] }],
    "no-restricted-globals" : ["warn", { "name" : "fdescribe", "message" : "Do not commit `fdescribes`. Use `describe` instead." }],
    "no-restricted-imports" : ["off", { "paths" : [], "patterns" : [] }],
    "no-restricted-properties" : ["warn", { "object" : "", "property" : "", "message" : "" }],
    "no-restricted-syntax" : ["warn"],
    "no-return-assign" : ["warn", "always"],
    "no-return-await" : ["off"],
    "no-script-url" : ["warn"],
    "no-self-assign" : ["warn", { "props" : true }],
    "no-self-compare" : ["warn"],
    "no-sequences" : ["warn", { "allowInParentheses" : false }],
    "no-setter-return" : ["warn"],
    "no-shadow-restricted-names" : ["warn"],
    "no-shadow" : ["off", { "builtinGlobals" : true, "ignoreOnInitialization" : false, "hoist" : "functions", "allow" : ["Clipboard", "require"] }],
    "no-sparse-arrays" : ["warn"],
    "no-tabs" : ["warn", { "allowIndentationTabs" : false }],
    "no-template-curly-in-string" : ["warn"],
    "no-ternary" : ["off"],
    "no-this-before-super" : ["warn"],
    "no-throw-literal" : ["off"],
    "no-trailing-spaces" : ["warn", { "ignoreComments" : false, "skipBlankLines" : false }],
    "no-undef-init" : ["off"],
    "no-undef" : ["warn", { "typeof" : true }],
    "no-undefined" : ["off"],
    "no-underscore-dangle" : ["warn", {
      "allowAfterSuper" : false,
      "allowAfterThis" : true,
      "allowAfterThisConstructor" : false,
      "allowFunctionParams" : true,
      "allowInArrayDestructuring" : true,
      "allowInObjectDestructuring" : true,
      "enforceInClassFields" : false,
      "enforceInMethodNames" : false,
      "allow" : []
    }],
    "no-unexpected-multiline" : ["warn"],
    "no-unmodified-loop-condition" : ["warn"],
    "no-unneeded-ternary" : ["warn", { "defaultAssignment" : true }],
    "no-unreachable-loop" : ["warn", { "ignore" : [] }],
    "no-unreachable" : ["warn"],
    "no-unsafe-finally" : ["warn"],
    "no-unsafe-negation" : ["warn", { "enforceForOrderingRelations" : true }],
    "no-unsafe-optional-chaining" : ["warn", { "disallowArithmeticOperators" : true }],
    "no-unused-expressions" : ["off", { "allowShortCircuit" : false, "allowTaggedTemplates" : false, "allowTernary" : false }],
    "no-unused-labels" : ["warn"],
    "no-unused-private-class-members" : ["warn"],
    "no-unused-vars" : ["off", {
      "ignoreRestSiblings" : false,
      "args" : "all",
      "caughtErrors" : "all",
      "vars" : "all",
      "argsIgnorePattern" : "_",
      "caughtErrorsIgnorePattern" : "",
      "destructuredArrayIgnorePattern" : "",
      "varsIgnorePattern" : ""
    }],
    "no-use-before-define" : ["off", { "allowNamedExports" : false, "classes" : true, "functions" : true, "variables" : true }],
    "no-useless-backreference" : ["warn"],
    "no-useless-call" : ["warn"],
    "no-useless-catch" : ["warn"],
    "no-useless-computed-key" : ["warn", { "enforceForClassMembers" : true }],
    "no-useless-concat" : ["warn"],
    "no-useless-constructor" : ["off"],
    "no-useless-escape" : ["warn"],
    "no-useless-rename" : ["warn", { "ignoreDestructuring" : false, "ignoreImport" : false, "ignoreExport" : false }],
    "no-useless-return" : ["warn"],
    "no-var" : ["warn"],
    "no-void" : ["warn", { "allowAsStatement" : false }],
    "no-warning-comments" : ["warn", { "location" : "anywhere", "terms" : ["BUG", "ISSUE", "TODO"] }],
    "no-whitespace-before-property" : ["warn"],
    "no-with" : ["warn"],
    "nonblock-statement-body-position" : ["warn", "beside"],
    "object-curly-newline" : ["warn", { "consistent" : true, "multiline" : true, "minProperties" : 10 }],
    "object-curly-spacing" : ["off", "always", { "arraysInObjects" : true, "objectsInObjects" : true }],
    "object-property-newline" : ["warn", { "allowAllPropertiesOnSameLine" : true }],
    "object-shorthand" : ["warn", "always", { "avoidExplicitReturnArrows" : true, "avoidQuotes" : false, "ignoreConstructors" : false, "methodsIgnorePattern" : "" }],
    "one-var-declaration-per-line" : ["warn", "always"],
    "one-var" : ["warn", { "separateRequires" : true, "const" : "never", "let" : "never", "var" : "never" }],
    "operator-assignment" : ["warn", "never"],
    "operator-linebreak" : ["warn", "after", { "overrides" : { "?" : "before", ":" : "before" } }],
    "padded-blocks" : ["warn", { "blocks" : "never", "classes" : "never", "switches" : "never" }],
    "padding-line-between-statements" : ["off", {
      "blankLine" : "always",
      "prev" : ["block", "block-like", "do", "for", "function", "if", "multiline-block-like", "return", "switch", "throw", "try", "while", "with"],
      "next" : ["block", "block-like", "do", "for", "function", "if", "multiline-block-like", "return", "switch", "throw", "try", "while", "with"]
    }],
    "prefer-arrow-callback" : ["warn", { "allowNamedFunctions" : false, "allowUnboundThis" : true }],
    "prefer-const" : ["warn", { "ignoreReadBeforeAssign" : false, "destructuring" : "any" }],
    "prefer-destructuring" : ["warn", {
      "AssignmentExpression" : { "array" : true, "object" : false },
      "VariableDeclarator" : { "array" : true, "object" : false }
    }, {
      "enforceForRenamedProperties" : true
    }],
    "prefer-exponentiation-operator" : ["warn"],
    "prefer-named-capture-group" : ["warn"],
    "prefer-numeric-literals" : ["warn"],
    "prefer-object-has-own" : ["warn"],
    "prefer-object-spread" : ["warn"],
    "prefer-promise-reject-errors" : ["warn", { "allowEmptyReject" : false }],
    "prefer-reflect" : ["warn", { "exceptions" : [] }],
    "prefer-regex-literals" : ["warn", { "disallowRedundantWrapping" : true }],
    "prefer-rest-params" : ["warn"],
    "prefer-spread" : ["warn"],
    "prefer-template" : ["warn"],
    "quote-props" : ["warn", "consistent-as-needed", { "keywords" : true, "numbers" : false, "unnecessary" : true }],
    "quotes" : ["off", "single", { "allowTemplateLiterals" : true, "avoidEscape" : true }],
    "radix" : ["warn", "always"],
    "require-atomic-updates" : ["warn", { "allowProperties" : false }],
    "require-await" : "off",
    "require-jsdoc" : ["warn", {
      "require" : { "ArrowFunctionExpression" : false, "ClassDeclaration" : false, "FunctionDeclaration" : true, "FunctionExpression" : false, "MethodDefinition" : false }
    }],
    "require-unicode-regexp" : ["off"],
    "require-yield" : ["warn"],
    "rest-spread-spacing" : ["warn", "never"],
    "semi-spacing" : ["warn", { "before" : false, "after" : true }],
    "semi-style" : ["warn", "last"],
    "semi" : ["off", "always", { "omitLastInOneLineBlock" : false }],
    "sort-imports" : ["warn", {
      "allowSeparatedGroups" : true,
      "ignoreCase" : false,
      "ignoreDeclarationSort" : false,
      "ignoreMemberSort" : false,
      "memberSyntaxSortOrder" : ["all", "multiple", "single", "none"]
    }],
    "sort-keys" : ["off", "asc", { "allowLineSeparatedGroups" : true, "caseSensitive" : true, "natural" : true, "minKeys" : 2 }],
    "sort-vars" : ["warn", { "ignoreCase" : false }],
    "space-before-blocks" : ["off", { "classes" : "always", "functions" : "always", "keywords" : "always" }],
    "space-before-function-paren" : ["off", { "anonymous" : "never", "asyncArrow" : "never", "named" : "never" }],
    "space-in-parens" : ["warn", "never"],
    "space-infix-ops" : ["off", { "int32Hint" : false }],
    "space-unary-ops" : ["warn", { "nonwords" : false, "words" : true }],
    "spaced-comment" : ["warn", "always", { "exceptions" : ["*"], "markers" : [] }],
    "strict" : ["warn", "safe"],
    "switch-colon-spacing" : ["warn", { "before" : true, "after" : true }],
    "symbol-description" : ["warn"],
    "template-curly-spacing" : ["warn", "always"],
    "template-tag-spacing" : ["warn", "always"],
    "unicode-bom" : ["warn", "never"],
    "use-isnan" : ["warn", { "enforceForIndexOf" : true, "enforceForSwitchCase" : true }],
    "valid-jsdoc" : ["warn", {
      "requireParamDescription" : true,
      "requireParamType" : false,
      "requireReturn" : false,
      "requireReturnDescription" : true,
