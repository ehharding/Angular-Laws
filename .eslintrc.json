{
  "root" : true,
  "env" : {
    "browser" : true,
    "es2020" : true,
    "node" : true
  },
  "parser" : "@typescript-eslint/parser",
  "parserOptions" : {
    "ecmaFeatures" : {
      "arrowFunctions" : true,
      "classes" : true,
      "forOf" : true,
      "globalReturn" : false,
      "impliedStrict" : true,
      "modules" : true,
      "spread" : true,
      "templateStrings" : true
    },
    "ecmaVersion" : 2020,
    "sourceType" : "module"
  },
  "plugins" : ["@typescript-eslint"],
  "rules" : {
    "accessor-pairs" : ["warn", { "enforceForClassMembers" : true, "getWithoutSet" : true, "setWithoutGet" : true }],
    "array-bracket-newline" : "off",
    "array-bracket-spacing" : ["warn", "never", { "arraysInArrays" : false, "objectsInArrays" : false, "singleValue" : false }],
    "array-callback-return" : ["warn", { "allowImplicit" : false, "checkForEach" : true }],
    "array-element-newline" : "off",
    "arrow-body-style" : ["warn", "always"],
    "arrow-parens" : ["warn", "always"],
    "arrow-spacing" : ["warn", { "before" : true, "after" : true }],
    "block-scoped-var" : "error",
    "block-spacing" : ["warn", "always"],
    "brace-style" : ["warn", "stroustrup", { "allowSingleLine" : false }],
    "camelcase" : ["warn", { "ignoreDestructuring" : false, "ignoreGlobals" : false, "properties" : "always", "allow" : [] }],
    "capitalized-comments" : ["warn", "always", { "ignoreConsecutiveComments" : true, "ignoreInlineComments" : true }],
    "class-methods-use-this" : ["warn", { "exceptMethods" : [] }],
    "comma-dangle" : ["warn", { "arrays" : "never", "imports" : "never", "exports" : "never", "functions" : "never", "objects" : "never" }],
    "comma-spacing" : ["warn", { "before" : false, "after" : true }],
    "comma-style" : ["warn", "last", {
      "exceptions" : {
        "ArrayExpression" : false,
        "ArrayFunctionExpression" : false,
        "ArrayPattern" : false,
        "CallExpression" : false,
        "FunctionDeclaration" : false,
        "FunctionExpression" : false,
        "ImportDeclaration" : false,
        "NewExpression" : false,
        "ObjectExpression" : false,
        "ObjectPattern" : false,
        "VariableDeclaration" : false
      }
    }],
    "complexity" : ["warn", { "max" : 10 }],
    "computed-property-spacing" : ["warn", "never", { "enforceForClassMembers" : true }],
    "consistent-return" : ["warn", { "treatUndefinedAsUnspecified" : false }],
    "consistent-this" : ["warn", "that"],
    "constructor-super" : "error",
    "curly" : ["warn", "all"],
    "default-case" : ["warn", { "commentPattern" : "/^no default$/i" }],
    "dot-location" : ["warn", "object"],
    "dot-notation" : ["warn", { "allowKeywords" : true }],
    "eol-last" : ["warn", "always"],
    "eqeqeq" : ["warn", "always"],
    "for-direction" : "error",
    "func-call-spacing" : ["warn", "never"],
    "func-name-matching" : ["warn", "always", { "considerPropertyDescriptor" : false, "includeCommonJSModuleExports" : false }],
    "func-names" : ["warn", "always"],
    "func-style" : ["warn", "declaration", { "allowArrowFunctions" : true }],
    "function-call-argument-newline" : ["warn", "never"],
    "function-paren-newline" : ["warn", { "minItems" : 5 }],
    "generator-star-spacing" : ["warn", "after"],
    "getter-return" : ["error", { "allowImplicit" : false }],
    "guard-for-in" : "warn",
    "id-denylist" : ["error", "callback", "cb", "e", "err"],
    "id-length" : ["warn", { "min" : 3, "max" : 30, "properties" : "always", "exceptions" : ["tmp"] }],
    "implicit-arrow-linebreak" : ["warn", "below"],
    "indent" : ["warn", 2],
    "init-declarations" : ["warn", "always"],
    "key-spacing" : ["warn", { "beforeColon" : true, "afterColon" : true, "mode" : "strict" }],
    "keyword-spacing" : ["warn", { "before" : true, "after" : true }],
    "linebreak-style" : ["warn", "windows"],
    "line-comment-position" : "off",
    "lines-around-comment" : ["warn", {
      "beforeBlockComment" : true,
      "afterBlockComment" : false,
      "beforeLineComment" : true,
      "afterLineComment" : false,
      "allowBlockStart" : false,
      "allowBlockEnd" : false,
      "allowObjectStart" : true,
      "allowObjectEnd" : false,
      "allowArrayStart" : false,
      "allowArrayEnd" : false,
      "allowClassStart" : true,
      "allowClassEnd" : false,
      "applyDefaultIgnorePatterns" : true
    }],
    "lines-around-directive" : ["warn", { "before" : "never", "after" : "always" }],
    "lines-between-class-members" : ["warn", "always", { "exceptAfterSingleLine" : false }],
    "max-depth" : ["warn", { "max" : 4 }],
    "max-len" : ["warn", {
      "ignoreComments" : false,
      "ignoreTrailingComments" : false,
      "ignoreUrls" : true,
      "ignoreStrings" : true,
      "ignoreTemplateLiterals" : true,
      "ignoreRegExpLiterals" : true,
      "code" : 150,
      "comments" : 150,
      "tabWidth" : 2
    }],
    "max-lines" : ["warn", { "skipBlankLines" : false, "skipComments" : false, "max" : 1000 }],
    "max-nested-callbacks" : ["warn", { "max" : 3 }],
    "max-params" : ["warn", { "max" : 3 }],
    "max-statements" : ["warn", { "max" : 150 }],
    "max-statements-per-line" : ["warn", { "max" : 1 }],
    "multiline-comment-style" : "off",
    "multiline-ternary" : ["warn", "never"],
    "new-cap" : ["warn", { "newIsCap" : true, "capIsNew" : false, "properties" : true, "newIsCapExceptions" : [], "capIsNewExceptions" : [] }],
    "new-parens" : ["warn", "always"],
    "newline-per-chained-call" : ["warn", { "ignoreChainWithDepth" : 5 }],
    "no-alert" : "warn",
    "no-array-constructor" : "warn",
    "no-await-in-loop" : "warn",
    "no-bitwise" : "warn",
    "no-caller" : "error",
    "no-case-declarations" : "error",
    "no-class-assign" : "error",
    "no-compare-neg-zero" : "warn",
    "no-cond-assign" : ["warn", "except-parens"],
    "no-confusing-arrow" : ["warn", { "allowParens" : true }],
    "no-console" : ["warn", { "allow" : ["error"] }],
    "no-const-assign" : "error",
    "no-constant-condition" : ["warn", { "checkLoops" : true }],
    "no-continue" : "warn",
    "no-control-regex" : "warn",
    "no-debugger" : "warn",
    "no-delete-var" : "warn",
    "no-div-regex" : "warn",
    "no-dupe-args" : "error",
    "no-dupe-class-members" : "error",
    "no-dupe-keys" : "error",
    "no-duplicate-case" : "error",
    "no-duplicate-imports" : ["warn", { "includeExports" : true }],
    "no-else-return" : ["warn", { "allowElseIf" : false }],
    "no-empty" : ["warn", { "allowEmptyCatch" : false }],
    "no-empty-character-class" : "warn",
    "no-empty-function" : ["warn", { "allow" : ["arrowFunctions", "constructors"] }],
    "no-empty-pattern" : "warn",
    "no-eq-null" : "warn",
    "no-eval" : ["warn", { "allowIndirect" : false }],
    "no-ex-assign" : "warn",
    "no-extend-native" : ["error", { "exceptions" : [] }],
    "no-extra-bind" : "warn",
    "no-extra-boolean-cast" : ["warn", { "enforceForLogicalOperands" : true }],
    "no-extra-label" : "warn",
    "no-extra-parens" : ["warn", "all", {
      "conditionalAssign" : true,
      "returnAssign" : true,
      "enforceForArrowConditionals" : true,
      "enforceForFunctionPrototypeMethods" : true,
      "enforceForNewInMemberExpressions" : true,
      "enforceForSequenceExpressions" : true,
      "nestedBinaryExpressions" : true,
      "ignoreJSX" : "none"
    }],
    "no-extra-semi" : "warn",
    "no-fallthrough" : ["warn", { "commentPattern" : "/^break[\\s\\w]*omitted$/i" }],
    "no-floating-decimal" : "warn",
    "no-func-assign" : "warn",
    "no-global-assign" : ["warn", { "exceptions" : [] }],
    "no-implicit-coercion" : "warn",
    "no-implicit-globals" : ["warn", { "lexicalBindings" : false }],
    "no-implied-eval" : "warn",
    "no-inline-comments" : "off",
    "no-inner-declarations" : ["warn", "both"],
    "no-invalid-regexp" : "warn",
    "no-invalid-this" : ["warn", { "capIsConstructor" : true }],
    "no-irregular-whitespace" : ["warn", { "skipComments" : true, "skipRegExps" : false, "skipStrings" : true, "skipTemplates" : true }],
    "no-iterator" : "error",
    "no-label-var" : "warn",
    "no-labels" : "error",
    "no-lone-blocks" : "warn",
    "no-lonely-if" : "warn",
    "no-loop-func" : "error",
    "no-loss-of-precision" : "error",
    "no-magic-numbers" : ["warn", {
      "detectObjects" : true,
      "enforceConst" : false,
      "ignoreArrayIndexes" : false,
      "ignoreDefaultValues" : true,
      "ignore" : [-2, -1, 0, 1, 2]
    }],
    "no-mixed-operators" : ["warn", {
      "allowSamePrecedence" : true,
      "groups" : [
        ["+", "-", "*", "/", "%", "**"],
        ["&", "|", "^", "~", "<<", ">>", ">>>"],
        ["==", "!=", "===", "!==", ">", ">=", "<", "<="],
        ["&&", "||"],
        ["in", "instanceof"]
      ]
    }],
    "no-mixed-spaces-and-tabs" : "warn",
    "no-multi-assign" : "warn",
    "no-multi-spaces" : ["warn", { "ignoreEOLComments" : false, "exceptions" : { "Property" : true } }],
    "no-multi-str" : "warn",
    "no-multiple-empty-lines" : ["warn", { "max" : 1, "maxBOF" : 0, "maxEOF" : 1 }],
    "no-negated-condition" : "warn",
    "no-nested-ternary" : "warn",
    "no-new" : "warn",
    "no-new-func" : "warn",
    "no-new-object" : "error",
    "no-new-symbol" : "error",
    "no-new-wrappers" : "error",
    "no-obj-calls" : "error",
    "no-octal" : "error",
    "no-octal-escape" : "error",
    "no-param-reassign" : ["warn", { "props" : true, "ignorePropertyModificationsFor" : [], "ignorePropertyModificationsForRegex" : [] }],
    "no-plusplus" : ["error", { "allowForLoopAfterthoughts" : true }],
    "no-proto" : "error",
    "no-prototype-builtins" : "error",
    "no-redeclare" : ["error", { "builtinGlobals" : true }],
    "no-regex-spaces" : "error",
    "no-restricted-globals" : ["warn", { "name" : "fdescribe", "message" : "Do Not Commit fdescribes. Use describe Instead." }],
    "no-return-assign" : ["warn", "always"],
    "no-return-await" : "warn",
    "no-script-url" : "warn",
    "no-self-assign" : ["warn", { "props" : true }],
    "no-self-compare" : "warn",
    "no-sequences" : "warn",
    "no-shadow" : ["warn", { "builtinGlobals" : true, "hoist" : "functions", "allow" : ["require"] }],
    "no-shadow-restricted-names" : "error",
    "no-sparse-arrays" : "warn",
    "no-tabs" : ["warn", { "allowIndentationTabs" : false }],
    "no-template-curly-in-string" : "warn",
    "no-ternary" : "off",
    "no-this-before-super" : "error",
    "no-throw-literal" : "warn",
    "no-trailing-spaces" : ["warn", { "ignoreComments" : false, "skipBlankLines" : false }],
    "no-undef" : "off",
    "no-undef-init" : "warn",
    "no-undefined" : "off",
    "no-underscore-dangle" : ["warn", {
      "allowAfterSuper" : false,
      "allowAfterThis" : true,
      "allowAfterThisConstructor" : false,
      "allowFunctionParams" : true,
      "enforceInMethodNames" : true,
      "allow" : []
    }],
    "no-unexpected-multiline" : "warn",
    "no-unmodified-loop-condition" : "warn",
    "no-unneeded-ternary" : ["warn", { "defaultAssignment" : true }],
    "no-unreachable" : "error",
    "no-unsafe-finally" : "error",
    "no-unsafe-negation" : ["warn", { "enforceForOrderingRelations" : true }],
    "no-unused-expressions" : ["warn", { "allowShortCircuit" : false, "allowTaggedTemplates" : false, "allowTernary" : false }],
    "no-unused-labels" : "warn",
    "no-unused-vars" : "off",
    "no-use-before-define" : ["warn", { "classes" : true, "functions" : true, "variables" : true }],
    "no-useless-call" : "warn",
    "no-useless-computed-key" : ["warn", { "enforceForClassMembers" : true }],
    "no-useless-concat" : "warn",
    "no-useless-constructor" : "warn",
    "no-useless-escape" : "warn",
    "no-useless-rename" : ["warn", { "ignoreDestructuring" : false, "ignoreImport" : false, "ignoreExport" : false }],
    "no-useless-return" : "warn",
    "no-var" : "error",
    "no-void" : ["error", { "allowAsStatement" : false }],
    "no-warning-comments" : ["warn", { "location" : "anywhere", "terms" : ["BUG", "ISSUE", "TODO"] }],
    "no-whitespace-before-property" : "warn",
    "no-with" : "error",
    "nonblock-statement-body-position" : ["warn", "beside"],
    "object-curly-newline" : ["warn", { "consistent" : true, "multiline" : true, "minProperties" : 5 }],
    "object-curly-spacing" : ["warn", "always", { "arraysInObjects" : false, "objectsInObjects" : false }],
    "object-property-newline" : ["warn", { "allowAllPropertiesOnSameLine" : true }],
    "object-shorthand" : ["warn", "always", { "avoidExplicitReturnArrows" : true, "avoidQuotes" : false, "ignoreConstructors" : false }],
    "one-var" : ["warn", { "separateRequires" : true, "const" : "never", "let" : "never" }],
    "one-var-declaration-per-line" : ["warn", "always"],
    "operator-assignment" : ["warn", "never"],
    "operator-linebreak" : ["warn", "after"],
    "padded-blocks" : ["warn", { "blocks" : "never", "classes" : "never", "switches" : "never" }],
    "prefer-arrow-callback" : ["warn", { "allowNamedFunctions" : false, "allowUnboundThis" : true }],
    "prefer-const" : ["warn", { "ignoreReadBeforeAssign" : false, "destructuring" : "any" }],
    "prefer-numeric-literals" : "error",
    "prefer-promise-reject-errors" : ["warn", { "allowEmptyReject" : false }],
    "prefer-rest-params" : "error",
    "prefer-spread" : "error",
    "prefer-template" : "error",
    "quote-props" : ["warn", "consistent-as-needed", { "keywords" : true, "numbers" : false, "unnecessary" : true }],
    "quotes" : ["warn", "single", { "allowTemplateLiterals" : true, "avoidEscape" : true }],
    "radix" : ["warn", "always"],
    "require-await" : "warn",
    "require-yield" : "error",
    "rest-spread-spacing" : ["warn", "never"],
    "semi" : ["warn", "always", { "omitLastInOneLineBlock" : false }],
    "semi-spacing" : ["warn", { "before" : false, "after" : true }],
    "semi-style" : ["error", "last"],
    "sort-imports" : ["warn", {
      "allowSeparatedGroups" : true,
      "ignoreCase" : false,
      "ignoreDeclarationSort" : false,
      "ignoreMemberSort" : false,
      "memberSyntaxSortOrder" : ["all", "multiple", "single", "none"]
    }],
    "sort-keys" : "off",
    "sort-vars" : ["warn", { "ignoreCase" : false }],
    "space-before-blocks" : ["warn", { "classes" : "always", "functions" : "always", "keywords" : "always" }],
    "space-before-function-paren" : ["warn", { "anonymous" : "never", "asyncArrow" : "never", "named" : "never" }],
    "space-in-parens" : ["warn", "never"],
    "space-infix-ops" : ["warn", { "int32Hint" : false }],
    "space-unary-ops" : ["warn", { "nonwords" : false, "words" : true }],
    "spaced-comment" : ["warn", "always", { "exceptions" : ["*"], "markers" : [] }],
    "strict" : ["error", "safe"],
    "switch-colon-spacing" : ["warn", { "before" : true, "after" : true }],
    "symbol-description" : "warn",
    "template-curly-spacing" : ["warn", "always"],
    "template-tag-spacing" : ["warn", "always"],
    "unicode-bom" : ["error", "never"],
    "use-isnan" : ["warn", { "enforceForIndexOf" : true, "enforceForSwitchCase" : true }],
    "valid-typeof" : ["warn", { "requireStringLiterals" : true }],
    "vars-on-top" : "warn",
    "wrap-iife" : ["warn", "outside", { "functionPrototypeMethods" : true }],
    "wrap-regex" : "warn",
    "yield-star-spacing" : ["warn", "after"],
    "yoda" : ["warn", "never", { "exceptRange" : false, "onlyEquality" : false }]
  },
  "overrides" : [
    {
      "files" : ["*.ts"],
      "parserOptions" : {
        "createDefaultProgram" : true,
        "project" : ["tsconfig.*?.json", "e2e/tsconfig.json"],
        "rules" : {
          "@angular-eslint/contextual-lifecycle" : "error",
          "@angular-eslint/no-attribute-decorator" : "warn",
          "@angular-eslint/no-lifecycle-call" : "warn",
          "@angular-eslint/no-output-native" : "warn",
          "@angular-eslint/no-pipe-impure" : "warn",
          "@angular-eslint/prefer-on-push-component-change-detection" : "warn",
          "@angular-eslint/template-accessibility-tabindex-no-positive" : "error",
          "@angular-eslint/template-banana-in-box" : "error",
          "@angular-eslint/template-no-autofocus" : "error",
          "@angular-eslint/template-no-negated-async" : "warn",
          "@angular-eslint/use-injectable-provided-in" : "warn",
          "@angular-eslint/use-lifecycle-interface" : "warn",
          "@angular-eslint/component-max-inline-declarations" : ["warn", { "animations" : 0, "styles" : 0, "template" : 0 }],
          "@angular-eslint/no-conflicting-lifecycle" : "warn",
          "@angular-eslint/no-forward-ref" : "warn",
          "@angular-eslint/no-input-prefix" : "off",
          "@angular-eslint/no-input-rename" : "warn",
          "@angular-eslint/no-output-on-prefix" : "warn",
          "@angular-eslint/no-output-rename" : "warn",
          "@angular-eslint/prefer-output-readonly" : "error",
          "@angular-eslint/relative-url-prefix" : "off",
          "@angular-eslint/template-cyclomatic-complexity" : ["warn", 10],
          "@angular-eslint/template-no-call-expression" : "warn",
          "@angular-eslint/use-component-selector" : "warn",
          "@angular-eslint/use-component-view-encapsulation" : "warn",
          "@angular-eslint/use-pipe-decorator" : "warn",
          "@angular-eslint/use-pipe-transform-interface" : "warn",
          "@angular-eslint/component-class-suffix" : "warn",
          "@angular-eslint/component-selector" : ["warn", "element", "iv", "kebab-case"],
          "@angular-eslint/directive-class-suffix" : "warn",
          "@angular-eslint/directive-selector" : ["warn", "attribute", "iv", "camelCase"],
          "@angular-eslint/no-host-metadata-property" : "warn",
          "@angular-eslint/no-inputs-metadata-property" : "warn",
          "@angular-eslint/no-outputs-metadata-property" : "warn",
          "@angular-eslint/no-queries-metadata-property" : "warn"
        }
      }
    },
    {
      "files" : ["*.component.ts"],
      "extends" : ["plugin:@angular-eslint/template/process-inline-templates"]
    },
    {
      "files" : ["*.spec.ts", "karma.conf.ts"],
      "rules" : {
        "no-magic-numbers" : "off"
      }
    }
  ]
}
