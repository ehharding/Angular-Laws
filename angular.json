{
  "$schema": "node_modules/@angular/cli/lib/config/schema.json",
  "newProjectRoot" : "projects",
  "version" : 1,
  "cli" : {
    "analytics" : "272fc58f-c451-448e-9d84-257489841d8e",
    "packageManager" : "npm",
    "cache" : { "enabled" : true, "environment" : "local", "path" : ".angular/cache" },
    "warnings" : { "versionMismatch" : true }
  },
  "projects" : {
    "pocket-fic" : {
      "schematics" : {
        "@schematics/angular:class" : { "skipTests" : false },
        "@schematics/angular:component" : {
          "flat" : true,
          "inlineStyle" : false,
          "inlineTemplate" : false,
          "skipImport" : false,
          "skipTests" : false,
          "changeDetection" : "OnPush",
          "prefix" : "pf",
          "style" : "scss",
          "viewEncapsulation" : "Emulated"
        },
        "@schematics/angular:directive" : { "flat" : true, "skipImport" : false, "skipTests" : false, "prefix" : "pf" },
        "@schematics/angular:module" : { "commonModule" : true, "flat" : false, "routing" : false, "routingScope" : "Child" },
        "@schematics/angular:pipe" : { "flat" : true, "skipImport" : false, "skipTests" : false },
        "@schematics/angular:service" : { "flat" : true, "skipTests" : false }
      },
      "prefix" : "pf",
      "projectType" : "application",
      "root" : "",
      "sourceRoot" : "src",
      "architect" : {
        "build" : {
          "builder" : "@angular-devkit/build-angular:browser-esbuild",
          "configurations" : {
            "production" : {
              "aot" : true,
              "buildOptimizer" : true,
              "commonChunk" : true,
              "deleteOutputPath" : true,
              "extractLicenses" : true,
              "namedChunks" : true,
              "preserveSymlinks" : false,
              "serviceWorker" : true,
              "subresourceIntegrity" : false,
              "vendorChunk" : false,
              "verbose" : false,
              "watch" : false,
              "baseHref" : "/Pocket-Fic/",
              "outputPath" : "dist-production",
              "index" : "src/index.html",
              "main" : "src/main.ts",
              "ngswConfigPath" : "ngsw-config.json",
              "polyfills" : "src/polyfills.ts",
              "tsConfig" : "tsconfig.app.json",
              "crossOrigin" : "anonymous",
              "inlineStyleLanguage" : "scss",
              "outputHashing" : "none",
              "allowedCommonJsDependencies" : ["lodash"],
              "assets" : ["src/assets", "src/favicon.ico", "src/manifest.webmanifest"],
              "styles" : ["src/styles.scss"],
              "budgets" : [
                { "type" : "initial", "maximumWarning" : "2MB", "maximumError" : "5MB" },
                { "type" : "anyComponentStyle", "maximumWarning" : "6kB", "maximumError" : "10kB" }
              ],
              "fileReplacements" : [{ "replace" : "src/environments/environment.development.ts", "with" : "src/environments/environment.production.ts" }],
              "optimization" : { "scripts" : true, "fonts" : { "inline" : true }, "styles" : { "inlineCritical" : true, "minify" : true } },
              "sourceMap" : { "hidden" : false, "scripts" : true, "styles" : true, "vendor" : true }
            }
          },
          "options" : {
            "aot" : true,
            "buildOptimizer" : true,
            "commonChunk" : true,
            "deleteOutputPath" : true,
            "extractLicenses" : false,
            "namedChunks" : true,
            "preserveSymlinks" : false,
            "serviceWorker" : false,
            "subresourceIntegrity" : false,
            "vendorChunk" : false,
            "verbose" : false,
            "watch" : false,
            "baseHref" : "/",
            "outputPath" : "dist",
            "index" : "src/index.html",
            "main" : "src/main.ts",
            "ngswConfigPath" : "ngsw-config.json",
            "polyfills" : "src/polyfills.ts",
            "tsConfig" : "tsconfig.app.json",
            "crossOrigin" : "anonymous",
            "inlineStyleLanguage" : "scss",
            "outputHashing" : "none",
            "allowedCommonJsDependencies" : ["lodash"],
            "assets" : ["src/assets", "src/favicon.ico", "src/manifest.webmanifest"],
            "styles" : ["src/styles.scss"],
            "budgets" : [
              { "type" : "initial", "maximumWarning" : "2MB", "maximumError" : "5MB" },
              { "type" : "anyComponentStyle", "maximumWarning" : "6kB", "maximumError" : "10kB" }
            ],
            "optimization" : { "scripts" : true, "fonts" : { "inline" : true }, "styles" : { "inlineCritical" : true, "minify" : true } },
            "sourceMap" : { "hidden" : false, "scripts" : true, "styles" : true, "vendor" : true },
            "stylePreprocessorOptions" : { "includePaths" : ["node_modules/bootstrap/scss/bootstrap.scss"] }
          }
        },
        "serve" : {
          "builder" : "@angular-devkit/build-angular:dev-server",
          "configurations" : { "production" : { "browserTarget" : "pocket-fic:build" } },
          "options" : { "browserTarget" : "pocket-fic:build" }
        },
        "extract-i18n" : {
          "builder" : "@angular-devkit/build-angular:extract-i18n",
          "options" : { "browserTarget" : "pocket-fic:build" }
        },
        "test" : {
          "builder" : "@angular-devkit/build-angular:karma",
          "options" : {
            "codeCoverage" : true,
            "sourceMap" : { "scripts" : true, "styles" : true, "vendor" : true },
            "karmaConfig" : "karma.conf.ts",
            "main" : "src/test.ts",
            "polyfills" : "src/polyfills.ts",
            "tsConfig" : "tsconfig.spec.json",
            "assets" : ["src/assets", "src/favicon.ico", "src/manifest.webmanifest"],
            "styles" : ["src/styles.scss"],
            "stylePreprocessorOptions" : { "includePaths" : ["node_modules/bootstrap/scss/bootstrap.scss"] }
          }
        },
        "lint" : {
          "builder" : "",
          "options" : { "exclude" : ["**/node_modules/**"], "tsConfig" : ["tsconfig.app.json"] }
        }
      },
      "i18n" : { "sourceLocale" : "en" }
    }
  }
}
