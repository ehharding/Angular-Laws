{
  "$schema" : "node_modules/@angular/cli/lib/config/schema.json",
  "defaultProject" : "internet-visualizer",
  "newProjectRoot" : "projects",
  "version" : 1,
  "cli" : { "analytics" : "272fc58f-c451-448e-9d84-257489841d8e", "packageManager" : "npm", "warnings" : { "versionMismatch" : true } },
  "projects" : {
    "internet-visualizer" : {
      "schematics" : {
        "@schematics/angular:class" : { "skipTests" : false },
        "@schematics/angular:component" : {
          "flat" : true,
          "inlineStyle" : false,
          "inlineTemplate" : false,
          "skipImport" : false,
          "skipTests" : false,
          "changeDetection" : "OnPush",
          "viewEncapsulation" : "Emulated",
          "prefix" : "iv",
          "style" : "scss"
        },
        "@schematics/angular:directive" : { "flat" : true, "skipImport" : false, "skipTests" : false, "prefix" : "iv" },
        "@schematics/angular:module" : { "commonModule" : true, "flat" : false, "routing" : false, "routingScope" : "Child" },
        "@schematics/angular:pipe" : { "flat" : true, "skipImport" : false, "skipTests" : false },
        "@schematics/angular:service" : { "flat" : true, "skipTests" : false }
      },
      "prefix" : "iv",
      "projectType" : "application",
      "root" : "",
      "sourceRoot" : "src",
      "architect" : {
        "build" : {
          "builder" : "@angular-devkit/build-angular:browser",
          "configurations" : {
            "production" : {
              "aot" : true,
              "buildOptimizer" : true,
              "commonChunk" : true,
              "deleteOutputPath" : true,
              "extractCss" : true,
              "extractLicenses" : true,
              "forkTypeChecker" : true,
              "namedChunks" : false,
              "preserveSymlinks" : false,
              "serviceWorker" : false,
              "showCircularDependencies" : true,
              "subresourceIntegrity" : true,
              "vendorChunk" : false,
              "verbose" : false,
              "watch" : false,
              "baseHref" : "internet-visualizer",
              "crossOrigin" : "none",
              "outputHashing" : "all",
              "allowedCommonJsDependencies" : ["lodash"],
              "budgets" : [
                { "type" : "initial", "maximumWarning" : "2MB", "maximumError" : "5MB" },
                { "type" : "anyComponentStyle", "maximumWarning" : "6kB", "maximumError" : "10kB" }
              ],
              "fileReplacements" : [{ "replace" : "src/environments/environment.ts", "with" : "src/environments/environment.prod.ts" }],
              "lazyModules" : [],
              "scripts" : [],
              "optimization" : { "scripts" : true, "styles" : true },
              "sourceMap" : { "hidden" : false, "scripts" : true, "styles" : true, "vendor" : true }
            }
          },
          "options" : {
            "outputPath" : "dist/internet-visualizer",
            "index" : "src/index.html",
            "main" : "src/main.ts",
            "polyfills" : "src/polyfills.ts",
            "tsConfig" : "tsconfig.app.json",
            "assets" : ["src/assets", "src/favicon.ico"],
            "styles" : [
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "src/assets/icons/github-logo.svg",
              "src/assets/themes/deeppurple-amber.css",
              "src/assets/themes/indigo-pink.css",
              "src/assets/themes/pink-bluegrey.css",
              "src/assets/themes/purple-green.css",
              "src/styles.scss",
              { "inject" : false, "bundleName" : "deeppurple-amber", "input" : "src/assets/themes/deeppurple-amber.css" },
              { "inject" : false, "bundleName" : "indigo-pink", "input" : "src/assets/themes/indigo-pink.css" },
              { "inject" : false, "bundleName" : "pink-bluegrey", "input" : "src/assets/themes/pink-bluegrey.css" },
              { "inject" : false, "bundleName" : "purple-green", "input" : "src/assets/themes/purple-green.css" }
            ]
          }
        },
        "serve" : {
          "builder" : "@angular-devkit/build-angular:dev-server",
          "configurations" : { "production" : { "aot" : true, "browserTarget" : "internet-visualizer:build:production" } },
          "options" : { "browserTarget" : "internet-visualizer:build" }
        },
        "extract-i18n" : {
          "builder" : "@angular-devkit/build-angular:extract-i18n",
          "options" : { "browserTarget" : "internet-visualizer:build" }
        },
        "test" : {
          "builder" : "@angular-devkit/build-angular:karma",
          "options" : {
            "codeCoverage": true,
            "sourceMap" : true,
            "karmaConfig" : "karma.conf.ts",
            "main" : "src/test.ts",
            "polyfills" : "src/polyfills.ts",
            "tsConfig" : "tsconfig.spec.json",
            "assets" : ["src/assets", "src/favicon.ico"],
            "scripts" : [],
            "styles" : [
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "src/assets/icons/github-logo.svg",
              "src/assets/themes/deeppurple-amber.css",
              "src/assets/themes/indigo-pink.css",
              "src/assets/themes/pink-bluegrey.css",
              "src/assets/themes/purple-green.css",
              "src/styles.scss"
            ]
          }
        },
        "lint" : {
          "builder" : "@angular-eslint/builder:lint",
          "options" : {
            "exclude" : ["**/node_modules/**"],
            "lineFilePatterns" : ["src/**/*.ts", "src/**/*.html"],
            "tsConfig" : ["tsconfig.app.json"]
          }
        }
      },
      "i18n" : { "sourceLocale" : "en-US" }
    }
  }
}
